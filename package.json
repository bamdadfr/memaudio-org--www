    "url": "https://bamdad.fr"
  },
  "scripts": {
    "build": "next build && yarn build:post",
    "build:post": "yarn build:post:remove-source-maps && yarn build:post:build-sitemap",
    "build:post:build-sitemap": "next-sitemap",
    "build:post:remove-source-maps": "find .next -type f -name \"*.map\" -exec rm {} \\;",
    "dev": "next dev",
    "export": "next export",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "package:clean": "rimraf .next && rimraf node_modules",
    "package:reinstall": "yarn package:clean && yarn",
    "package:sort": "npx sort-package-json",
    "start": "next start",
    "test": "yarn jest",
    "test:coverage": "jest --coverage && codecov",
    "test:experimental": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "jest --watch",
    "test:watch:all": "jest --watchAll"
  },
  "dependencies": {
    "@react-spring/web": "^9.5.1",
    "immer": "^9.0.6",
    "just-shuffle": "^4.0.1",
    "next": "^12.1.5",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-icons": "^4.2.0",
    "react-player": "^2.9.0",
    "react-use": "^17.3.1",
    "styled-components": "^5.3.0",
    "use-sound": "^4.0.1",
    "zustand": "^4.0.0-rc.1"
  },
  "devDependencies": {
    "@bamdadsabbagh/eslint-config": "^2.0.25",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.1.1",
    "@testing-library/react-hooks": "^8.0.0",
    "@testing-library/user-event": "^14.1.1",
    "@types/react": "^18.0.8",
    "@types/react-dom": "^18.0.3",
    "@types/styled-components": "^5.1.12",
    "babel-jest": "^28.0.3",
    "babel-plugin-styled-components": "^2.0.2",
    "codecov": "^3.8.3",
    "cross-env": "^7.0.3",
    "eslint-config-next": "^12.0.3",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-testing-library": "^5.0.0",
    "file-loader": "^6.2.0",
    "jest": "^28.0.3",
    "jest-environment-jsdom": "^28.0.2",
    "jest-fetch-mock": "^3.0.3",
    "next-sitemap": "^2.5.20",
    "rimraf": "^3.0.2",
    "sass": "^1.51.0",
    "sass-reset": "^1.0.2",
    "typescript-plugin-styled-components": "^2.0.0",
    "url-loader": "^4.1.1"
  }
}
